{
  "name": "@marijnnn/mcp-nuclino",
  "version": "0.8.0",
  "description": "MCP server for Nuclino integration",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "start": "TRANSPORT_TYPE=http node dist/index.js",
    "start:stdio": "TRANSPORT_TYPE=stdio node dist/index.js",
    "dev": "TRANSPORT_TYPE=http  ts-node --esm src/index.ts",
    "dev:stdio": "TRANSPORT_TYPE=stdio ts-node --esm src/index.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "mcp", "model-context-protocol",
    "server",
    "nuclino",
    "http"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.15",
    "express": "^5.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.13",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "bin": {
    "mcp-nuclino": "./dist/index.js"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  }
}
